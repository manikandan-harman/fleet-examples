apiVersion: v1
kind: PersistentVolumeClaim
metadata:
  name: ekuiper-pvc
  namespace: industrialqa-ns
spec:
  accessModes:
  - ReadWriteOnce
  resources:
    requests:
      storage: 1Gi
  storageClassName: local-path
---
